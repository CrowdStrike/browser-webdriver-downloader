{
  "name": "microsoft-edgedriver",
  "version": "1.3.0",
  "description": "Install and wrap msedgedriver in Node.js",
  "bin": {
    "msedgedriver": "bin/msedgedriver.js"
  },
  "main": "src",
  "files": [
    "bin",
    "src"
  ],
  "keywords": [
    "msedgedriver",
    "webdriver"
  ],
  "scripts": {
    "lint:git": "commitlint --default-branch main",
    "lint:js": "eslint . --ext js,json",
    "lint:md": "remark -f README.md",
    "install": "node bin/install-msedgedriver",
    "start": "node bin/msedgedriver.js",
    "release": "standard-version",
    "test": "mocha"
  },
  "standard-version": {
    "scripts": {
      "posttag": "git push --follow-tags --atomic"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/CrowdStrike/microsoft-edgedriver.git"
  },
  "author": "Kelly Selden",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CrowdStrike/microsoft-edgedriver/issues"
  },
  "homepage": "https://github.com/CrowdStrike/microsoft-edgedriver#readme",
  "engines": {
    "node": ">=14.15"
  },
  "dependencies": {
    "execa": "^5.0.0",
    "extract-zip": "^2.0.1",
    "fs-extra": "^10.1.0",
    "got": "^11.8.5",
    "tmp": "0.2.1"
  },
  "devDependencies": {
    "@crowdstrike/commitlint": "^5.0.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-fs": "^2.0.0",
    "eslint": "^8.0.0",
    "eslint-config-crowdstrike": "^8.0.0",
    "eslint-config-crowdstrike-node": "^3.0.0",
    "eslint-plugin-json-files": "^1.0.0",
    "eslint-plugin-mocha": "^10.0.0",
    "eslint-plugin-node": "^11.0.0",
    "mocha": "^10.0.0",
    "mocha-helpers": "^6.2.1",
    "remark-cli": "^11.0.0",
    "remark-preset-lint-crowdstrike": "^2.0.0",
    "renovate-config-standard": "^2.0.0",
    "standard-node-template": "3.0.0",
    "standard-version": "9.5.0"
  }
}
